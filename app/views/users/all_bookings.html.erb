<h1>All bookings</h1>

<div class="container">
	<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">UserID</th>
      <th scope="col">hotelID</th>
      <th scope="col">fromDate</th>
      <th scope="col">Todate</th>
      <th scope="col">TotalPrice</th>
    </tr>
  </thead>
  <tbody>
  	<% @bookings.each do |b| %>
    <tr>
      <th scope="row">b.id</th>
      <td>b.user_id</td>
      <td>b.hotel_detail_id</td>
      <td>b.from_date</td>
      <td>b.to_date</td>
      <td>b.total_price</td>
    </tr>
   <% end %>
  </tbody>
</table>
</div>